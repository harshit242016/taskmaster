{
    "version": 2,
    "builds": [
      { "src": "index.js", "use": "@vercel/static" },
      { "src": "server/services/server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/.*", "dest": "server/services/server.js" },
      { "src": "/(.*)", "dest": "index.html" }
    ]
}
  